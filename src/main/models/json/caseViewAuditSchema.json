{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "classpath:/JsonSchema/case_view_audit_schema.json",
  "type": "object",
  "title": "The LAU Case View Audit Schema",
  "required": [
    "viewLog",
    "startRecordNumber",
    "moreRecords"
  ],
  "properties": {
    "actionLog": {
      "id": "#viewLog",
      "type": "array",
      "items": { "$ref": "caseViewLogSchema.json" },
      "title": "Array of case view logs"
    },
    "startRecordNumber": {
      "id": "#startRecordNumber",
      "type": "integer",
      "minimum": 1,
      "title": "The index of the first record out of the full result set provided"
    },
    "moreRecords": {
      "id": "#moreRecords",
      "type": "boolean",
      "title": "Indicates whether there are more records beyond the current page in the full result set"
    }
  }
}
